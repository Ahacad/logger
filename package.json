{
  "name": "@ahacad/logger",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.1.1",
  "description": "A lightweight, colorful, and feature-rich JavaScript logger",
  "keywords": [
    "log",
    "logger",
    "logging",
    "debug",
    "trace",
    "console"
  ],
  "homepage": "https://github.com/Ahacad/logger#readme",
  "bugs": {
    "url": "https://github.com/Ahacad/logger/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ahacad/logger.git"
  },
  "license": "MIT",
  "author": "ahacad",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --watch",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "example:cjs": "node example.cjs",
    "example:esm": "node example.mjs"
  },
  "dependencies": {
    "eslint-scope": "^7.2.2",
    "eslint-visitor-keys": "^3.4.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14.16.0"
  },
  "module": "./dist/index.js",
  "sideEffects": false
}
